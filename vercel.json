{
    "routes": [
      {
        "src": "/sitemap.xml",
        "dest": "/sitemap.xml",
        "headers": {
          "Content-Type": "application/xml"
        }
      },
      {
        "src": "/robots.txt",
        "dest": "/robots.txt",
        "headers": {
          "Content-Type": "text/plain"
        }
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }