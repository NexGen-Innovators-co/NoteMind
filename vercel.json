{
    
    "headers": [
      {
        "source": "/robots.txt",
        "headers": [
          {
            "key": "Content-Type", 
            "value": "text/plain"
          }
        ]
      },
      {
        "source": "/favicon.ico",
        "headers": [
          {
            "key": "Content-Type", 
            "value": "image/x-icon"
          }
        ]
      },
      {
        "source": "/sitemap.xml",
        "headers": [
          {
            "key": "Content-Type", 
            "value": "application/xml"
          }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, s-maxage=0, must-revalidate"
          }
        ]
      }
    ]
  }
